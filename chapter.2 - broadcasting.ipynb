   "source": [
    "# broadcasting \n",
    "\n",
    "from sklearn.datasets import load_iris\n",
    "\n",
    "iris = load_iris()\n",
    "\n",
    "x1 = iris.data[0,:]\n",
    "\n",
    "v1 = np.array([x1])\n",
    "\n",
    "print(v1 - 2*np.ones((4,1)))\n",
    "\n",
    "# mean-removed vector \n",
    "\n",
    "x = 0\n",
    "v1_mean = 0\n",
    "\n",
    "for i in x1:\n",
    "    x += i \n",
    "    v1_mean = x / len(x1)\n",
    "\n",
    "print(v1 - v1_mean)"
   ]
