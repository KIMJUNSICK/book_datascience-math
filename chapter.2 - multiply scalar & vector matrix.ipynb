    "# multiply scalar & vector/matrix\n",
    "\n",
    "from sklearn.datasets import load_iris\n",
    "\n",
    "iris = load_iris()\n",
    "\n",
    "x1 = iris.data[0,:]\n",
    "\n",
    "v1 = np.array([x1])\n",
    "\n",
    "print(2*v1)\n",
    "print(3*v1)\n",
    "\n",
    "x2 = iris.data[1,:]\n",
    "x3 = iris.data[2,:]\n",
    "\n",
    "m1 = np.array([x2, x3])\n",
    "\n",
    "print(2*m1)\n",
    "print(3*m1)"
   ]
  }
